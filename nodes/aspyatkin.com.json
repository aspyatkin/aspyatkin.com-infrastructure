{
  "environment": "production",
  "run_list": [
    "recipe[aspyatkin-com::python2]",
    "recipe[latest-git::default]",
    "recipe[dotfiles::default]",
    "recipe[dhparam::default_key]",
    "recipe[modern_nginx::default]",
    "recipe[aspyatkin-com::default]"
  ],
  "automatic": {
    "ipaddress": "aspyatkin.com"
  },
  "dhparam": {
    "default_key_size": 2048
  },
  "dotfiles": {
    "users": [
      "root",
      "titan"
    ]
  },
  "rbenv": {
    "group_users": [
      "root",
      "titan"
    ]
  },
  "aspyatkin-com": {
    "user": "titan",
    "fqdn": "aspyatkin.com"
  }
}
