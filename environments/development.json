{
  "name": "development",
  "description": "Development environment",
  "json_class": "Chef::Environment",
  "chef_type": "environment",
  "default_attributes": {
    "build-essential": {
      "compile_time": true
    },
    "dhparam": {
      "default_key_size": 2048
    },
    "latest-nodejs": {
      "install": "current",
      "binary": true
    },
    "nsd": {
      "master": {
        "fqdn": "ns1.e17k.local",
        "ip_address": "172.16.0.21",
        "contact": "hostmaster@e17k.local"
      },
      "slaves": {
        "ns2.e17k.local": "172.16.0.22"
      },
      "zones": [
        "e17k.local"
      ]
    },
    "ntp": {
      "servers": [
        "0.pool.ntp.org",
        "1.pool.ntp.org",
        "2.pool.ntp.org",
        "3.pool.ntp.org"
      ]
    },
    "modern_nginx": {
      "install": "mainline"
    }
  }
}
